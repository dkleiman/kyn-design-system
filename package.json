    "react-dom": "^16.8.6",
    "react-scripts": "^3.0.1",
    "react-styleguidist": "^9.1.2",
    "rimraf": "^2.6.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
